<div class="musician-card">
  <div class="musician-picture">
    <%= image_tag musician.photo, height: "150"%>
  </div>
  <div class="musician-info">
    <h2><%= musician.first_name %> <%= musician.last_name %></h2>
    <h4>Ensemble type: <%= musician.ensemble_type %></h4>
    <h4>Instruments: <%= musician.instruments %></h4>
    <h3>$<%= musician.price %></h3><p> per concert</p>
    <%= link_to "learn more",  musician_path(musician) %>
    <%= link_to "Leave a review", new_user_review_path(musician) %>

    <%= link_to "learn more",  musician_path(musician), class: "btn btn-primary" %>
    <br>
    <br>

  </div>
</div>
